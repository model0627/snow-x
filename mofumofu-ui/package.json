		"rehype-stringify": "^10.0.1",
		"remark-breaks": "^4.0.0",
		"remark-emoji": "^5.0.2",
		"remark-gfm": "^4.0.1",
		"remark-github-blockquote-alert": "^2.0.0",
		"remark-math": "^6.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"remark-toc": "^9.0.0",
		"svelte": "^5.38.7",
		"svelte-check": "^4.3.1",
		"svelte-easy-crop": "^4.0.1",
		"svelte-hero-icons": "^5.2.0",
		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^2.1.0",
		"tailwindcss": "^4.1.13",
